[user]
	name = Keith Salisbury
	email = <keithsalisbury@gmail.com>
	signingkey = 229704FE
[color]
  ui = true
[core]
  editor = vim
	excludesfile = ~/.gitignore_global
[push]
	default = current
[alias]
	co = checkout
  cp = cherry-pick
	br = branch -v
	ci = commit
  s  = status
	st = status
  a  = add -p
  i  = add -i
  g  = grep --break --heading --line-number
  d  = diff
  pf = push -f
  l  = log --oneline --graph --decorate
  ll = log -p
  lt = log trunk
  recent = for-each-ref --sort=-committerdate refs/heads/ --count=15 --format='%(HEAD)%(objectname:short) %(color:yellow)%(refname:short)%(color:reset) - %(color:green)%(committerdate:relative)%(color:reset)'
  brx = for-each-ref --sort=committerdate refs/heads/ --format='%(HEAD) %(color:yellow)%(refname:short)%(color:reset) - %(color:red)%(objectname:short)%(color:reset) - %(contents:subject) - %(authorname) (%(color:green)%(committerdate:relative)%(color:reset))'
[commit]
  template = ~/.commitmessage
[grep]
	extendRegexp = true
	lineNumber = true
[fetch]
	prune = true
